{
  "name": "packy-usage",
  "displayName": "%displayName%",
  "description": "%description%",
  "version": "1.1.1",
  "publisher": "MashiroWang",
  "pricing": "Free",
  "author": {
    "name": "mashirowang",
    "email": "github@te.sb"
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/94mashiro/packy-usage-vsce"
  },
  "bugs": {
    "url": "https://github.com/94mashiro/packy-usage-vsce/issues"
  },
  "homepage": "https://github.com/94mashiro/packy-usage-vsce#readme",
  "license": "MIT",
  "keywords": [
    "api",
    "budget",
    "monitoring",
    "usage",
    "cost",
    "packy"
  ],
  "engines": {
    "vscode": "^1.54.0"
  },
  "l10n": "./l10n",
  "categories": [
    "Other",
    "Visualization"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "packy-usage.setToken",
        "title": "%command.setToken.title%",
        "category": "Packy Usage"
      },
      {
        "command": "packy-usage.refresh",
        "title": "%command.refresh.title%",
        "category": "Packy Usage",
        "icon": "$(refresh)"
      },
      {
        "command": "packy-usage.showExplorer",
        "title": "%command.showExplorer.title%",
        "category": "Packy Usage"
      }
    ],
    "configuration": {
      "title": "%config.title%",
      "properties": {
        "packy-usage.apiToken": {
          "type": "string",
          "default": "",
          "description": "%config.apiToken.description%",
          "scope": "application"
        },
        "packy-usage.apiEndpoint": {
          "type": "string",
          "default": "https://www.packycode.com/api/backend/users/info",
          "description": "%config.apiEndpoint.description%",
          "scope": "application"
        },
        "packy-usage.proxy": {
          "type": "string",
          "default": "",
          "description": "要使用的代理设置。如果未设置，则将从http_proxy和https_proxy环境变量继承",
          "scope": "application"
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "packy-usage",
          "title": "%view.container.title%",
          "icon": "$(package)"
        }
      ]
    },
    "views": {
      "packy-usage": [
        {
          "id": "packy-usage.explorer",
          "name": "%view.explorer.name%",
          "when": "true"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "packy-usage.refresh",
          "when": "view == packy-usage.explorer",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "vscode-test",
    "package": "npm run vsce -- package --allow-missing-repository"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.54.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/l10n-dev": "^0.0.35",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.6.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-prettier": "^5.5.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0"
  },
  "dependencies": {
    "undici": "^6.21.3"
  }
}
